    "test": "./node_modules/.bin/jest"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "fs-extra": "^3.0.1",
    "hyperquest": "^2.1.2",
    "rear-core": "0.2.2",
    "rear-logger": "^0.2.0",
    "semver": "^5.3.0",
    "tar-pack": "^3.4.0",
    "tmp": "^0.0.33",
    "leven": "^2.1.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "jest": "^20.0.4",
    "rear-logger": "0.1.1",
    "rear-system-package": "^0.1.0"
  },
  "jest": {
    "moduleFileExtensions": ["js"],
    "moduleDirectories": ["node_modules"],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
